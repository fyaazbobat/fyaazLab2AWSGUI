<Application x:Class="fyaazLab2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:fyaazLab2"
             xmlns:viewModel="clr-namespace:fyaazLab2"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModel:Login}">
          
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:PdfList}">
            <local:PdfView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewModel:Book}">
            <local:ShowPdf/>
        </DataTemplate>
    </Application.Resources>
</Application>
